<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
	<title>PyroCMS v2.2-dev API</title>
    <parser>
        <default-package-name>No Package</default-package-name>
    </parser>
    <files>
        <directory>.</directory>
        <ignore>addons/*</ignore>
        <ignore>*/language/*</ignore>
        <ignore>*/codeigniter/*</ignore>
        <ignore>*/installer/*</ignore>
        <ignore>*/themes/*</ignore>
        <ignore>*/views/*</ignore>
        <ignore>*/migrations/*</ignore>
        <ignore>*/sparks/*</ignore>
        <ignore>*/logs/*</ignore>
        <ignore>*/errors/*</ignore>
        <ignore>*/config/*.php</ignore>
    </files>
    <plugins>
        <plugin path="Core">
            <option name="a">value</option>
        </plugin>
    </plugins>
    <logging>
        <level>warn</level>
    </logging>
</phpdoc>